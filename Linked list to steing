#include <bits/stdc++.h>
using namespace std;

struct Node {
    char data;
    Node* next;
    Node(char x) {
        data = x;
        next = NULL;
    }
};

string linkedListToString(Node* head) {
    string result = "";
    Node* temp = head;

    while (temp != NULL) {
        result.push_back(temp->data);
        temp = temp->next;
    }

    return result;
}

int main() {
    // Example: H -> e -> l -> l -> o
    Node* head = new Node('H');
    head->next = new Node('e');
    head->next->next = new Node('l');
    head->next->next->next = new Node('l');
    head->next->next->next->next = new Node('o');

    cout << linkedListToString(head) << endl; // Output: Hello
    return 0;
}
